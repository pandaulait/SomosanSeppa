<% case @result.quiz_type %>
<% when "SelectionQuiz" then %>
  <%= render 'public/results/sq_show',{ quiz: @quiz, result: @result} %>
<% when "DescriptiveQuiz" then %>
  <%= render 'public/results/dq_show',{ quiz: @quiz, result: @result, entities: @entities} %>
<% end %>
