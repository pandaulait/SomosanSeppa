<h1>Public::Quizzes#index</h1>
<p>Find me in app/views/public/quizzes/index.html.erb</p>



  <table class="table">
    <thead>
      <th>ID</th>
      <th>作者</th>
      <th>問題文</th>
      <th>説明</th>
    </thead>
    <tbody>
      <% @quizzes.each do |quiz| %>
        
          <tr>
            <td><%= link_to quiz_path(quiz) do %><%= quiz.id %><% end %></td>
            <td><%= quiz.user.name %></td>
            <td><%= quiz.content %></td>
            <td><%= quiz.explanation %></td>
          </tr>
        
      <% end %>
    </tbody>

  </table>

